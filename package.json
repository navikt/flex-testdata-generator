    "dependencies": {
        "@js-joda/core": "^5.4.2",
        "@navikt/ds-css": "^1.3.21",
        "@navikt/ds-react": "^1.3.21",
        "@navikt/ds-tokens": "^1.3.21",
        "jsonschema": "^1.4.1",
        "kafkajs": "^2.2.2",
