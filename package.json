        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "eslint": "^8.26.0",
        "eslint-config-next": "^13.0.0",
        "eslint-plugin-cypress": "^2.12.1",
        "next": "^12.3.1",
